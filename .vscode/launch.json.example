    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Tunnelmole",
            "program": "${workspaceFolder}/dist/src/index.js",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "args": [
                "8001",
            ],
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ],
            "console":"internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        },
    ]
}